{
	"tabWidth": 2,
	"semi": false,
	"singleQuote": true,
	"plugins": ["prettier-plugin-tailwindcss"],
	"tailwindStylesheets": "./src/styles/index.css",
	"useTabs": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"htmlWhitespaceSensitivity": "ignore",
	"singleAttributePerLine": true,
	"bracketSameLine": false,
	"printWidth": 80,
	"proseWrap": "always",
	"trailingComma": "none"
}
